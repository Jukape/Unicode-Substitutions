{
    "name": "unicode-substitutions",
    "displayName": "Unicode-Substitutions",
    "description": "Corrects common unicode substitutions such as quotes and hyphens.",
    "version": "0.0.1",
    "publisher": "GlenBuktenica",
    "license": "MIT",
    "engines": {
        "vscode": "^1.15.0"
    },
    "categories": [
        "Linters"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
        "contributes": {
            "languages": [
                {
                    "id": "foo-lang",
                    "aliases": [
                        "Foo"
                    ],
                    "extensions": [
                        "foo"
                    ]
                }
            ],
            "commands": [
                {
                    "command": "extension.format-foo",
                    "title": "(Bad) Format Foo Files"
                }
            ]
        },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "vscode": "^1.1.17",
        "typescript": "^3.0.3",
        "@types/node": "7.0.4"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/gbuktenica/Unicode-Substitutions.git"
    }
}
